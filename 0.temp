//7. Reverse Integer


class Solution {
public:
    int reverse(int x) {
        if(x==0)return 0;
        int f=0;
        if(x<0)f=1;
        int num=x,d=0,rev=0;max=INT_MAX,min=INT_MIN;
         while(num!=0){
          d=x%10;
          if(num<10&&f=1)d*=-1;
          rev=rev*10+d;
         }
      if(rev>max||rev<min)return 0;  
    }
};
